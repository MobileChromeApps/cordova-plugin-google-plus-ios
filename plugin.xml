<plugin xmlns="http://cordova.apache.org/ns/plugins/1.0"
    id="cordova-plugin-google-plus-ios"
    version="1.7.2">
  <engines>
    <engine name="cordova" version=">=3.0.0" />
  </engines>

  <name>Google Plus iOS Framework Plugin</name>
  <keywords>google,plus,iOS,framework</keywords>
  <repo>https://github.com/MobileChromeApps/cordova-plugin-google-plus-ios</repo>
  <issue>https://github.com/MobileChromeApps/cordova-plugin-google-plus-ios/issues</issue>

  <platform name="ios">
    <dependency id="cordova-plugin-google-open-source-ios" />
    <framework src="GooglePlus.framework" custom="true" />
    <framework src="AddressBook.framework" />
    <framework src="AssetsLibrary.framework" />
    <framework src="Foundation.framework" />
    <framework src="CoreLocation.framework" />
    <framework src="CoreMotion.framework" />
    <framework src="CoreGraphics.framework" />
    <framework src="CoreText.framework" />
    <framework src="MediaPlayer.framework" />
    <framework src="Security.framework" />
    <framework src="SystemConfiguration.framework" />
    <framework src="UIKit.framework" />
  </platform>
</plugin>
